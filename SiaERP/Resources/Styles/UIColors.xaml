<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:theme="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2">
    
    <!-- Background colors -->
	<SolidColorBrush x:Key="C_BkgPrimary">#303C44</SolidColorBrush>
	<SolidColorBrush x:Key="C_BkgPrimaryOver">#566166</SolidColorBrush>
	<SolidColorBrush x:Key="C_BkgSecondary">#F0F0F0</SolidColorBrush>
	
	<!-- Font colors -->
    <SolidColorBrush x:Key="C_FntPrymary">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="C_FntSecondary">#000000</SolidColorBrush>

    <FontFamily x:Key="FntPrimary">Arial</FontFamily>
    <System:Double x:Key="FntSize">16</System:Double>
</ResourceDictionary>